                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button13" mnemonicParsing="false" GridPane.columnIndex="1"
                    GridPane.rowIndex="2">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>

            <Button fx:id="button2" mnemonicParsing="false" GridPane.columnIndex="2"
