<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.*?>
<ScrollPane fx:id="mainView" fitToHeight="true" fitToWidth="true" styleClass="bordered"
            xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.acepe.streamdeck.ui.MainViewController">

    <VBox maxHeight="1.7976931348623157E308" styleClass="spaced">
        <HBox alignment="CENTER_LEFT" minHeight="-Infinity" minWidth="-Infinity" prefHeight="70.0"
              styleClass="content-pane" VBox.vgrow="NEVER">

            <BorderPane maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                <right>
                    <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#onSettingsPerformed"
                            BorderPane.alignment="CENTER">
                        <styleClass>
                            <String fx:value="settings-button"/>
                            <String fx:value="square-button"/>
                        </styleClass>
                    </Button>
                </right>
            </BorderPane>
        </HBox>
        <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
            <styleClass>
                <String fx:value="deck-grid"/>
            </styleClass>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
            </rowConstraints>

            <Button fx:id="button4" mnemonicParsing="false" GridPane.columnIndex="0"
                    GridPane.rowIndex="0">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button9" mnemonicParsing="false" GridPane.columnIndex="0"
                    GridPane.rowIndex="1">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button14" mnemonicParsing="false" GridPane.columnIndex="0"
                    GridPane.rowIndex="2">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>

            <Button fx:id="button3" mnemonicParsing="false" GridPane.columnIndex="1"
                    GridPane.rowIndex="0">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button8" mnemonicParsing="false" GridPane.columnIndex="1"
