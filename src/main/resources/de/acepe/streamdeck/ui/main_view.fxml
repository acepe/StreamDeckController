                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>

            <Button fx:id="button0" mnemonicParsing="false" GridPane.columnIndex="4"
                    GridPane.rowIndex="0">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button5" mnemonicParsing="false" GridPane.columnIndex="4"
                    GridPane.rowIndex="1">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button10" mnemonicParsing="false" GridPane.columnIndex="4"
