                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button7" mnemonicParsing="false" GridPane.columnIndex="2"
                    GridPane.rowIndex="1">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>
            <Button fx:id="button12" mnemonicParsing="false" GridPane.columnIndex="2"
                    GridPane.rowIndex="2">
                <styleClass>
                    <String fx:value="deck-button"/>
                </styleClass>
            </Button>

            <Button fx:id="button1" mnemonicParsing="false" GridPane.columnIndex="3"
